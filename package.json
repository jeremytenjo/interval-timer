{
  "name": "my-interval-timer",
  "description": "Handy app that helps you keep track of work and rest periods during workouts",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm ./devtools/scripts/dev.ts",
    "dev:app": "node --loader ts-node/esm ./devtools/vite/scripts/startDevServer.ts",
    "dev:storybook": "start-storybook -c ./devtools/storybook -p 6007 -s ./public --quiet --ci",
    "dev:prod": "node --loader ts-node/esm ./devtools/vite/scripts/runBuild.ts",
    "build": "node --loader ts-node/esm ./devtools/vite/scripts/buildApp.ts",
    "emulators:start": "firebase emulators:start --only auth,firestore",
    "emulators:firestore:export-data": "firebase emulators:export src/data/firestoreEmulatorData",
    "android:run": "npm run android:sync && cap run android",
    "android:open": "npm run android:sync && cap open android",
    "android:sync": "npm run build && cap sync android",
    "android:build-prod": "ENV=production npm run android:sync",
    "ios:open": "npm run ios:sync && cap open ios",
    "ios:sync": "npm run build && cap sync ios",
    "capacitor:updateConfig": "npx cap copy",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "tests:run": "./devtools/scripts/tests/run",
    "prettify": "prettier \"**/*.js\" --write --config ./devtools/prettier/config.json --ignore-path ./devtools/prettier/.prettierignore",
    "lint": "eslint ./ --fix && tsc --noemit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@capacitor-community/keep-awake": "^2.1.0",
    "@capacitor/app": "^1.0.6",
    "@capacitor/storage": "^1.2.3",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/lab": "^5.0.0-alpha.54",
    "@mui/material": "^5.1.0",
    "@useweb/array-db": "0.0.1",
    "@useweb/capitalize": "0.0.1",
    "@useweb/remove-last-character": "0.0.1",
    "@useweb/use-async": "^0.2.0",
    "@useweb/use-install-prompt": "^1.0.1",
    "@useweb/use-on-true": "0.0.1",
    "firebase": "^9.4.1",
    "format-duration": "^1.4.0",
    "history": "^5.1.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "react": "^17.0.2",
    "react-countdown-circle-timer": "^2.5.4",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.5",
    "react-router-dom": "^6.0.2",
    "swr": "^1.0.1",
    "ts-node": "^10.4.0",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@capacitor/android": "^3.3.1",
    "@capacitor/cli": "^3.3.1",
    "@capacitor/core": "^3.3.1",
    "@capacitor/ios": "^3.3.1",
    "@check-light-or-dark/color": "^1.0.3",
    "@storybook/addon-controls": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-react": "^1.0.9",
    "chalk": "^4.1.2",
    "concurrently": "^6.4.0",
    "css-loader": "^6.5.1",
    "cypress": "^9.0.0",
    "dotenv": "^10.0.0",
    "dotenv-parse-variables": "^2.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "filehound": "^1.17.5",
    "format-duration": "^1.4.0",
    "husky": "^7.0.4",
    "live-server": "^1.2.1",
    "lodash": "^4.17.21",
    "my-local-ip": "^1.0.0",
    "node-emoji": "^1.11.0",
    "prettier": "^2.4.1",
    "react-refresh": "^0.11.0",
    "rollup-plugin-manifest-json": "^1.5.1",
    "storybook": "^6.3.12",
    "style-loader": "^3.3.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-pwa": "^0.11.3"
  }
}
