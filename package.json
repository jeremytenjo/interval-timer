{
  "name": "my-interval-timer",
  "description": "Handy app that helps you keep track of your work and rest periods during workouts",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node ./devtools/scripts/dev",
    "dev:app": "node ./devtools/webpack/scripts/startDevServer",
    "dev:storybook": "start-storybook -c ./devtools/storybook -p 6007 -s ./public --quiet --ci",
    "dev:prod": "node ./devtools/webpack/scripts/startDevServer",
    "emulators:start": "firebase emulators:start --only auth,firestore",
    "emulators:firestore:export-data": "firebase emulators:export src/data/firestoreEmulatorData",
    "build": "node ./devtools/webpack/scripts/buildApp",
    "build:analyze": "node ./devtools/webpack/scripts/analyzeBundle",
    "android:run": "npm run android:sync && cap run android",
    "android:open": "npm run android:sync && cap open android",
    "android:sync": "npm run build && cap sync android",
    "ios:open": "npm run ios:sync && cap open ios",
    "ios:sync": "npm run build && cap sync ios",
    "capacitor:updateConfig": "npx cap copy",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "tests:run": "./devtools/scripts/tests/run",
    "serve": "node ./devtools/webpack/scripts/serveBuiltApp",
    "prettify": "prettier \"**/*.js\" --write --config ./devtools/prettier/config.js --ignore-path ./devtools/prettier/.prettierignore",
    "lint": "eslint ./ --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@capacitor-community/keep-awake": "^2.1.0",
    "@capacitor/storage": "^1.2.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@mui/lab": "^5.0.0-alpha.49",
    "@mui/material": "^5.0.2",
    "@useweb/array-db": "0.0.1",
    "@useweb/use-async": "0.0.1",
    "@useweb/use-on-true": "0.0.1",
    "firebase": "^9.0.0",
    "format-duration": "^1.4.0",
    "history": "^5.0.1",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-countdown-circle-timer": "^2.5.4",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.3",
    "swr": "^1.0.1",
    "zustand": "^3.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@capacitor/android": "^3.1.2",
    "@capacitor/cli": "^3.1.2",
    "@capacitor/core": "^3.1.2",
    "@capacitor/geolocation": "^1.1.0",
    "@capacitor/ios": "^3.0.0",
    "@capacitor/status-bar": "^1.0.3",
    "@capacitor/toast": "^1.0.3",
    "@check-light-or-dark/color": "^1.0.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@storybook/addon-controls": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "babel-loader": "^8.2.2",
    "babel-plugin-auto-import": "^1.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "cypress": "^8.4.0",
    "dotenv-webpack": "^7.0.3",
    "error-overlay-webpack-plugin": "^1.0.0",
    "eslint": "^7.20.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "filehound": "^1.17.4",
    "format-duration": "^1.4.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "live-server": "^1.2.1",
    "lodash": "^4.17.21",
    "my-local-ip": "^1.0.0",
    "node-emoji": "^1.10.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^6.0.0-beta.3",
    "storybook": "^6.2.9",
    "style-loader": "^3.2.1",
    "webpack": "^5.23.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-dev-server": "^4.2.1",
    "webpack-pwa-manifest": "^4.3.0"
  }
}
